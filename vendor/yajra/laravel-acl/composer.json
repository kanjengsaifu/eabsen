{
    "name": "yajra/laravel-acl",
    "description": "Laravel ACL is a simple role, permission ACL for Laravel Framework.",
    "keywords": [
        "yajra",
        "laravel",
        "acl",
        "user",
        "role",
        "permissions"
    ],
    "homepage": "https://github.com/yajra/laravel-acl",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com",
            "homepage": "http://yajrabox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.4",
        "illuminate/support": "~5.3",
        "illuminate/database": "~5.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Yajra\\Acl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\Acl\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\Acl\\AclServiceProvider"
            ]
        }
    }
}
